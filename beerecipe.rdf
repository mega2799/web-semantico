        <hops:alphaAcidsContent>11.5–14.0</hops:alphaAcidsContent>
        <hops:betaAcidsContent>4.2–5.8</hops:betaAcidsContent>
        <hops:perceivedAroma xml:lang="en">Herbal, Peach, Pear, Spicy</hops:perceivedAroma>
        <hops:perceivedAroma xml:lang="fr">Épicé, Herbes, Pêche, Poire</hops:perceivedAroma>
