    <!-- http://www.beerecipe.com/ontology#hasBeerStyle -->

    <owl:ObjectProperty rdf:about="http://www.beerecipe.com/ontology#hasBeerStyle">
        <rdfs:domain rdf:resource="https://rdf.ag/o/beer#Beer"/>
        <rdfs:range rdf:resource="http://www.beerecipe.com/ontology#BeerStyle"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.beerecipe.com/ontology#isProducedBy"/>
            <rdf:Description rdf:about="http://www.beerecipe.com/ontology#hasStyle"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>Style of the beer, inferred from the recipe that produces it</rdfs:comment>
        <rdfs:label>has beer style</rdfs:label>
    </owl:ObjectProperty>
    


