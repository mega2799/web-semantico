    <!-- https://rdf.ag/o/hops#HopsEastKentGolding -->

    <owl:Class rdf:about="https://rdf.ag/o/hops#HopsEastKentGolding">
        <rdfs:subClassOf rdf:resource="https://rdf.ag/o/hops#HopsGolding"/>
