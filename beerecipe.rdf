    <!-- https://mega2799.github.io/web-semantico/index-en.html#Recipe -->

    <owl:Class rdf:about="https://mega2799.github.io/web-semantico/index-en.html#Recipe">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://mega2799.github.io/web-semantico/index-en.html#Recipe"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://mega2799.github.io/web-semantico/index-en.html#hasCereal"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="https://mega2799.github.io/web-semantico/index-en.html#Cereal"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://mega2799.github.io/web-semantico/index-en.html#hasHop"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="https://rdf.ag/o/hops#Hops"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://mega2799.github.io/web-semantico/index-en.html#hasMalt"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="https://rdf.ag/o/malt#Malt"/>
                    </owl:Restriction>
                    <owl:Restriction>
