        <rdfs:label xml:lang="en">Amber Durum Wheat</rdfs:label>
        <rdfs:label xml:lang="fr">Bl Dur Ambr</rdfs:label>
        <skos:definition xml:lang="fr">Bl dont moins de 60% de grains sont constitus de grains vitreux et durs de bl dur de couleur ambre.</skos:definition>
        <skos:definition xml:lang="en">Wheat which has less than 60% of kernels consisting of vitreous and hard kernels of amber colored durum.</skos:definition>
        <prov:hadPrimarySource rdf:resource="https://rdf.ag/o/cerealstoo#USDA2014StandardsforWheat"/>
