        <rdfs:comment xml:lang="en">Step to stop enzymatic activity and raise mash temperature before lautering.</rdfs:comment>
        <rdfs:label>MashOut Step</rdfs:label>
    </owl:Class>
    


    <!-- https://mega2799.github.io/web-semantico/index-en.html#MashStep -->

