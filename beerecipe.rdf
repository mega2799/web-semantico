                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="https://mega2799.github.io/web-semantico/index-en.html#Water"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://mega2799.github.io/web-semantico/index-en.html#hasYeast"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onClass rdf:resource="https://mega2799.github.io/web-semantico/index-en.html#Yeast"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://mega2799.github.io/web-semantico/index-en.html#hasStyle"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://mega2799.github.io/web-semantico/index-en.html#BeerStyle"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A beer recipe describes the complete process and ingredients needed to produce a specific type of beer. It includes information about malts, hops, yeast, water chemistry, brewing steps, and expected characteristics of the final product.</rdfs:comment>
        <rdfs:label xml:lang="en">Recipe</rdfs:label>
        <skos:definition xml:lang="en">A structured set of instructions and ingredients for brewing beer, including quantities, temperatures, timings, and expected beer characteristics such as ABV, IBU, and color.</skos:definition>
        <skos:example xml:lang="en">Easy Pale Ale: A beginner-friendly recipe using pale ale malt, Cascade hops, producing a 4.8% ABV beer with 30 IBU.</skos:example>
    </owl:Class>
    


