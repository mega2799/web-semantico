        <rdfs:comment xml:lang="en">Rinse the grains to extract remaining sugars and increase wort yield.</rdfs:comment>
        <rdfs:label>Sparge Step</rdfs:label>
    </owl:Class>
    


    <!-- http://www.beerecipe.com/ontology#Water -->

    <owl:Class rdf:about="http://www.beerecipe.com/ontology#Water"/>
    


