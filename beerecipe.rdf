        <rdfs:subClassOf rdf:resource="https://rdf.ag/o/cerealstoo#WinterWheat"/>
        <void:inDataset rdf:resource="https://rdf.ag/o/cerealstoo#datasetdefinition"/>
        <rdfs:isDefinedBy rdf:resource="https://rdf.ag/o/cerealstoo-2021-02-18"/>
        <rdfs:label xml:lang="fr">Bl Rouge d&apos;Hiver de l&apos;Ouest Canadien</rdfs:label>
        <rdfs:label xml:lang="en">Canada Western Red Winter</rdfs:label>
        <skos:definition xml:lang="fr">Les Bls Rouges d&apos;Hiver de l&apos;Ouest Canadien ont trois grades de mouture et une trs bonne qualit de mouture. Il est couramment utilis pour la cuisson de pains plats et franais, de nouilles et de pains cuits  la vapeur.</skos:definition>
        <skos:definition xml:lang="en">The CWRW wheat class has three milling grades and very good milling quality. These wheats are commonly used for baking flat and french breads, noodles, and for steamed breads.</skos:definition>
        <prov:hadPrimarySource rdf:resource="https://rdf.ag/o/cerealstoo#CanadianGrainCommission2019Canadian-Wheat-Classes"/>
        <foaf:name>CWRW</foaf:name>
    </owl:Class>
    


    <!-- https://rdf.ag/o/cerealstoo#CWSWS -->

    <owl:Class rdf:about="https://rdf.ag/o/cerealstoo#CWSWS">
