        <rdfs:label xml:lang="en">Target</rdfs:label>
        <rdfs:label xml:lang="en">Wye Target</rdfs:label>
        <skos:definition xml:lang="fr">Le houblon Target, un descendant de Northern Brewer et d&apos;Eastwell Golding, a été élevé par le Dr Ray A. Neve en 1965 au Wye College au Royaume-Uni.</skos:definition>
        <skos:definition xml:lang="en">Target hops, a descendant of Northern Brewer and Eastwell Golding, was bred by Dr. Ray A. Neve in 1965 at Wye College in the UK.</skos:definition>
        <foaf:name>Target</foaf:name>
        <hops:alphaAcidsContent>8-12.5</hops:alphaAcidsContent>
        <hops:betaAcidsContent>5-5.5</hops:betaAcidsContent>
        <hops:perceivedAroma xml:lang="en">Cedar, Liquorice, Pine</hops:perceivedAroma>
        <hops:perceivedAroma xml:lang="fr">Cèdre, Pin, Réglisse</hops:perceivedAroma>
        <hops:totalOilContent>1.6-2.6</hops:totalOilContent>
    </owl:Class>
    


    <!-- https://rdf.ag/o/hops#HopsTarget_VF -->

    <owl:Class rdf:about="https://rdf.ag/o/hops#HopsTarget_VF">
        <rdfs:subClassOf rdf:resource="https://rdf.ag/o/hops#HopsTarget"/>
        <void:inDataset rdf:resource="https://rdf.ag/o/hops#datasetdefinition"/>
