        <hops:perceivedAroma xml:lang="en">Coconut, Fruity, Lemon, Lime Peel, Melon, Pineapple, Spicy</hops:perceivedAroma>
        <hops:totalOilContent>1.2-1.4</hops:totalOilContent>
    </owl:Class>
    


    <!-- https://rdf.ag/o/hops#HopsCeleia -->

    <owl:Class rdf:about="https://rdf.ag/o/hops#HopsCeleia">
